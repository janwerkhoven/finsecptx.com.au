Add...
No
2/4 -->

    <section id="super-funds">
      <div>
        <p>Big table</p>
        <ul>
          <li><button class="circle next">{{> arrow-right}}</button></li>
        </ul>
      </div>
    </section>

    <section id="objectives">
      <div>
        <p>Objectives</p>
        <ul>
          <li><button class="circle next">{{> arrow-right}}</button></li>
        </ul>
      </div>
    </section>

    <section id="end-part-two">
      <div>
        {{> chevron-down}}
        <p>Part 2. Complete!</p>
        <ul>
          <li><button class="next">Proceed to part 3 of 3</button></li>
        </ul>
      </div>
    </section>

    <section id="uk-address">
      <div>
        <div class="field text select">
          <label>What was your last UK address?</label>
          <input type="text" name="STREET1">
          <input type="text" name="STREET2">
          <input type="text" name="SUBURB">
          <div>
            <select name="STATE">
              <option selected>Choose...</option>
              <option value="ACT">Australian Capital Territory</option>
              <option value="NSW">New South Wales</option>
              <option value="NT">Northern Territory</option>
              <option value="QLD">Queensland</option>
              <option value="SA">South Australia</option>
              <option value="TAS">Tasmania</option>
              <option value="VIC">Victoria</option>
              <option value="WA">Western Australia</option>
            </select>
            <div>
              {{> chevron-down}}
            </div>
          </div>
          <input type="text" name="POSTCODE">
        </div>
      </div>
    </section>

    <section id="uk-pension">
      <div>
        <p>UK Pension</p>
        <ul>
          <li><button class="circle next">{{> arrow-right}}</button></li>
        </ul>
      </div>
    </section>

    <section id="end-part-three">
      <div>
        {{> chevron-down}}
        <p>Part 3. Complete!</p>
        <ul>
          <li><button class="next">Proceed to sign off</button></li>
        </ul>
      </div>
    </section>

    <section id="important">
      <div>
        <p><strong>Important</strong></p>
        <p>If you have more than one fund you must complete a separate Letter of Authority (contained in the next section) for each fund.</p>
        <ul>
          <li><button class="circle next">{{> arrow-right}}</button></li>
        </ul>
      </div>
    </section>

    <section id="letter-of-authority">
      <div>
        <p><strong>Letter of authority</strong></p>
        <p><small>To whom it may concern,</small></p>
        <p><small>Please accept this letter as my authority for you to release information concerning my pension entitlement to:</small></p>
        <p><small>FinSec Partners<br>
          T: +61 8 8357 7840<br>
          F: +61 8 8373 7600<br>
          PO Box 1096 Unley SA 5061.<br>
          E: ukpensions@finsecpartners.com.au</small></p>
        <p><small>Please also accept this form as my authority for you to update your records to reflect my correct address details as shown below:</small></p>
        <hr>
        <dl>
          <dt>Name</dt>
          <dd>Mr Scott Noell</dd>
          <dt>Date of birth</dt>
          <dd>16/6/1964</dd>
          <dt>Address</dt>
          <dd>Level 1
            <br> 10 Greenhill Rd
            <br> Wayville
            <br> SA 5061</dd>
          <dt>Last UK address</dt>
          <dd>Level 1
            <br> 10 Greenhill Rd
            <br> Wayville
            <br> SA 5061</dd>
          <dt>Name of UK Pension</dt>
          <dd>Example</dd>
          <dt>Address of UK Pension</dt>
          <dd>Level 1
            <br> 10 Example St
            <br> Wayville
            <br> SA 5061</dd>
          <dt>Period of fund or scheme</dt>
          <dd>May 1984 - May 1996</dd>
