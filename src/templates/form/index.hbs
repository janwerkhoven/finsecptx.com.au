    <section id="super-fund-x">
      <div>
        <div class="field text currency">
          <label>What is the estimated balance at <span name="SUPER_FUND_1">Your Super Fund #1</span>?</label>
          <input type="text" name="SUPER_FUND_1_BALANCE" tabindex="1">
          <span>$</span>
        </div>
        <div class="field option" name="SUPER_FUND_1_OWNER">
          <label>Who is the owner?</label>
          <div>
            <button tabindex="2">Myself</button>
